
<div id="<%= dom_id survey %>">


<div class="card">
  <div class="card-header">
   <%= survey.title %> 
  </div>
  <div class="card-body">
    <h5 class="card-title"> </h5>
   <div class="m-3"> <p class="card-text"> <%= survey.description %> </p> </div>

   <hr>

<div class="container text-center">
<div class="row">
 <div class="col-sm"> <%= link_to "Edit", edit_survey_path(survey), class: "btn btn-dark bi bi-plus-circle"%>  </div>
 <div class="col-sm"> <%= link_to "Take Survey", edit_survey_path(survey), class: "btn btn-success bi bi-clipboard-check" %> </div>
 <div class="col-sm">  <%= link_to "Design", edit_survey_path(survey), class: "btn btn-warning bi bi-palette"%>  </div>
 <div class="col-sm"> <%= button_to "Delete", survey, method: :delete, class: "btn btn-danger bi bi-trash" %>  </div>
 
</div>
</div>
</div>

</div> 
</div>  
  
    


</div>


